{
    "version": 2,
    "builds": [
        {
            "src": "app/src/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "app/src/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "app/src/server.js"
        }
    ]
}