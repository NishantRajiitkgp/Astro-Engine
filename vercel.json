{
    "version": 2,
    "builds": [
        {
            "src": "app/api/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "app/public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "app/api/index.js"
        },
        {
            "src": "/report",
            "dest": "app/public/report.html"
        },
        {
            "src": "/(.*\\.css)",
            "dest": "app/public/$1"
        },
        {
            "src": "/(.*\\.js)",
            "dest": "app/public/$1"
        },
        {
            "src": "/",
            "dest": "app/public/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "app/public/$1"
        }
    ]
}